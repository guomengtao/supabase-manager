{
  "name": "supabase-manager",
  "version": "1.1.0",
  "description": "A web-based Supabase management tool",
  "scripts": {
    "test": "jest --coverage --passWithNoTests",
    "test:watch": "jest --watch",
    "lint": "eslint .",
    "serve": "http-server . -p 3000"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/guomengtao/supabase-manager.git"
  },
  "keywords": [
    "supabase",
    "management",
    "database",
    "web"
  ],
  "author": "guomengtao",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/guomengtao/supabase-manager/issues"
  },
  "homepage": "https://github.com/guomengtao/supabase-manager#readme",
  "devDependencies": {
    "@babel/core": "^7.22.0",
    "@babel/preset-env": "^7.22.0",
    "@testing-library/dom": "^9.3.0",
    "@testing-library/jest-dom": "^5.16.5",
    "babel-jest": "^29.5.0",
    "eslint": "^8.41.0",
    "http-server": "^14.1.1",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.24.0"
  },
  "jest": {
    "testEnvironment": "jsdom",
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/js/$1"
    },
    "setupFilesAfterEnv": [
      "<rootDir>/jest.setup.js"
    ],
    "collectCoverageFrom": [
      "js/**/*.js",
      "!js/vendor/**",
      "!**/node_modules/**"
    ],
    "coverageReporters": [
      "text",
      "lcov",
      "json"
    ]
  }
}
